    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "clean": "npx rimraf -- hooks node_modules package-lock.json && npm i",
    "start.web.little-manufacturer-web": "nx serve web-little-manufacturer-web",
    "build.ionic.little-manufacturer": "nx build ionic-little-manufacturer",
    "prepare.ionic.little-manufacturer": "npm run clean && npm run clean.ionic.little-manufacturer && npm run build.ionic.little-manufacturer",
    "prepare.ionic.little-manufacturer.ios": "npm run prepare.ionic.little-manufacturer && cd apps/ionic-little-manufacturer && npm run cap.add.ios",
    "prepare.ionic.little-manufacturer.android": "npm run prepare.ionic.little-manufacturer && cd apps/ionic-little-manufacturer && npm run cap.add.android",
    "open.ionic.little-manufacturer.ios": "cd apps/ionic-little-manufacturer && npm run cap.ios",
    "open.ionic.little-manufacturer.android": "cd apps/ionic-little-manufacturer && npm run cap.android",
    "sync.ionic.little-manufacturer": "cd apps/ionic-little-manufacturer && npm run cap.copy",
    "clean.ionic.little-manufacturer": "cd apps/ionic-little-manufacturer && npx rimraf -- hooks node_modules platforms www plugins ios android package-lock.json && npm i && rimraf -- package-lock.json"
  },
